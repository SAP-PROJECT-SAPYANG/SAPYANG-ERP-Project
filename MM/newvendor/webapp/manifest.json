    "rootView": {
      "viewName": "b0315MM.newvendor.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "config": {
			"sample": {
				"stretch": true,
				"files": [
					"Main.view.xml",
					"Main.controller.js",
					"Change.fragment.xml",
					"Display.fragment.xml",
